    dcterms:identifier "ag-c6db0105efb6990ddf765a511bf149ff" ;
    dcterms:title "地点"@ja ;
    foaf:homepage "http://chiten.org/" ;
    foaf:name "Chiten"@en,
        "地点"@ja .

