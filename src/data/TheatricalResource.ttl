        "現代劇"@ja ;
    dcterms:temporal "2014/10/11" ;
    dcterms:title "My Father, Giacometti"@en,
        "わが父、ジャコメッティ"@ja ;
    dcterms:type "http://purl.org/dc/dcmitype/Event" .

