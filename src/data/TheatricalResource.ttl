    dcterms:identifier "03699-01-2017-02" ;
    dcterms:temporal "2017/12/1" ;
    dcterms:title "Five Days in March Re-creation"@en,
        "『三月の5日間』リクリエーション"@ja ;
    dcterms:type "http://purl.org/dc/dcmitype/Event" .

