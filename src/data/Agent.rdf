    <o:resource_class rdf:resource="http://enpaku-jdta.jp/api/resource_classes/307"/>
    <dcterms:identifier>ag-c7ed7b7267d90c64fff4e5da8519aa54</dcterms:identifier>
    <foaf:name xml:lang="en">Showmanship</foaf:name>
    <dcepk:location xml:lang="ja">福岡県</dcepk:location>
    <o:resource_template rdf:resource="http://enpaku-jdta.jp/api/resource_templates/12"/>
    <foaf:homepage>http://www.showman.jp/</foaf:homepage>
