  </o:Item>
  <dcepk:Agent rdf:about="http://enpaku-jdta.jp/api/items/208450">
    <o:title>株式会社劇団民藝</o:title>
    <o:site rdf:resource="http://enpaku-jdta.jp/api/sites/1"/>
