    <foaf:homepage>https://kioihall.jp/</foaf:homepage>
    <dcepk:agentTranscription xml:lang="ja">こうえきざいだんほうじんにっぽんせいてつぶんかざいだん</dcepk:agentTranscription>
    <o:is_public rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</o:is_public>
