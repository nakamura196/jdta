    "dcepk:titleTranscription": [
        {
            "@language": "ja",
            "@value": "みゅーじかるおんみょうじおおえやまへん",
            "is_public": true,
            "property_id": 517,
            "property_label": "公演名よみ",
            "type": "literal"
        }
