    "dcterms:abstract": [
        {
            "@language": "ja",
            "@value": "人通りの少ない道沿いの家から全く見知らぬ男が出て来る。この街の誰も知らない男だ。様子がおかしく、言葉も不自然である。その男にはじめに話しかけた通行人は、思いきり殴られ、卒倒する。",
            "is_public": true,
            "property_id": 19,
            "property_label": "Abstract",
            "type": "literal"
        },
        {
            "@language": "en",
            "@value": "A complete stranger comes out of a house along a road with minimal traffic. He is a stranger to everyone in the town. His appearance is strange and his words are unnatural. The first passerby who talked to him is punched and knocked out.",
            "is_public": true,
            "property_id": 19,
            "property_label": "Abstract",
            "type": "literal"
