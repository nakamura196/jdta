    "dcepk:titleTranscription": [
        {
            "@language": "ja",
            "@value": "れくちゃーぱふぉーまんす だんすはたいいくなり そのいち たいいくきょういんとしてのおおのかずおをとおして",
            "is_public": true,
            "property_id": 517,
            "property_label": "公演名よみ",
            "type": "literal"
        }
    ],
    "dcterms:abstract": [
        {
            "@language": "ja",
            "@value": "ダンスがなぜ体育なのか、なぜ女子＝ダンスなのかなど、体育教員の経験から感じた疑問を問い直すレクチャー・パフォーマンス。やはり体育教員だった大野一雄を軸に、幻のラジオ体操等に触れながら日本のダンスの来歴を振り返る。Dance Archive Project2016にて初演。",
            "is_public": true,
            "property_id": 19,
            "property_label": "Abstract",
            "type": "literal"
        },
        {
            "@language": "en",
            "@value": "A lecture performance asking questions posed by the experiences of a PE teacher such as “why is dance physical education?” and “why does dance=girls?”. Focussing on Kazuo Ohno, who was a PE teacher, it looks back at the history of dance in Japan along with phantom radio calisthenics. Premiered as the Dance Archive Project 2016.",
            "is_public": true,
            "property_id": 19,
            "property_label": "Abstract",
            "type": "literal"
        }
    ],
