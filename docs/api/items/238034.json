    "dcterms:abstract": [
        {
            "@language": "ja",
            "@value": "人と人が在る中に生まれる縁。人と関わることによって変化していく世界観。\r\n絡まり、千切れたり、先入観や見解に縛られた足かせをほどいて、\r\n隣にあるその縁を再び結んでいく。",
            "is_public": true,
            "property_id": 19,
            "property_label": "Abstract",
            "type": "literal"
        },
        {
            "@language": "en",
            "@value": "Connections born between people.\r\nPerspectives that change due to interactions with other.\r\nInterlacing, cutting off, untying the prejudices and opinions that bind our feel together, and reconnecting with those by our side.",
            "is_public": true,
            "property_id": 19,
            "property_label": "Abstract",
            "type": "literal"
        }
