    "dcterms:abstract": [
        {
            "@language": "ja",
            "@value": "登場人物達のそれぞれの善意が、互いをすり減らし、こそぎながら、“信じるに値するもの”を求めて、右往左往する様を“笑い”を交えて描く群像劇。",
            "is_public": true,
            "property_id": 19,
            "property_label": "Abstract",
            "type": "literal"
        },
        {
            "@language": "en",
            "@value": "An ensemble drama in which the good intentions of each of the characters wear and scrape down the others as they search for “that which is worthy of belief” and going back and forth with “laughter.”",
            "is_public": true,
            "property_id": 19,
            "property_label": "Abstract",
            "type": "literal"
        }
